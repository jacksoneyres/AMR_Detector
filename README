Requires Docker

SETUP

sudo docker run --name postgres -e POSTGRES_PASSWORD=biohazard -e POSTGRES_USER=admin -e POSTGRES_DB=mydb -e PGDATA=/var/lib/postgresql/data/pgdata -v /home/eyresj/pgdata:/var/lib/postgresql/data/pgdata --restart=always -d postgres
sudo docker run -d --restart=always --hostname rabbit --name rabbit rabbitmq:3
sudo docker run -v ~/test:/app/documents -p 0.0.0.0:8000:8000 --link rabbit:rabbit --link postgres:postgres jacksoneyres/amr
-detector


